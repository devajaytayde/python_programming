#For Loop with range -
 for i = 1 to range(10)
    print(i)

#For Loop without start -
 for i in range(10):
    print(i)

#For Loop with Start, Stop -
  for i in range(1,10):
    print(i)

#For Loop with Start, Stop, Step -
  for i in range(1,10,2):
    print(i)



#For checking Even or Odd number
  for i in range(1, 10):
      num = i%2
      if num==0:
        print(f"{num} is even")
      else:
        print(f"{num} is odd")



